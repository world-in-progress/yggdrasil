            "type": "object",  // Custom nested structure
            "fields": {
              "source": {"type": "string", "required": false},
              "tags": {"type": "array", "item_type": "string"}
            }
          },
          "owner": {"type": "BaseNode", "required": true}  // Complex type, use BaseNode as type
        }
      },
      {
        "name": "ComplexNode",
        "fields": {
          "data": {"type": "DataNode"},  // Nested DataNode Model
          "extra": {"type": "string"}
        }
      }
    ]
  }